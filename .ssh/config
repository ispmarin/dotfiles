Host *
    ServerAliveInterval 300
    ServerAliveCountMax 2
    IdentitiesOnly yes

Host ma.sdf.org
    User mfactor
	IdentityFile ~/.ssh/id_rsa_sdf

Host tty.sdf.org
    User mfactor
	IdentityFile ~/.ssh/id_rsa_sdf

Host github.com
	IdentityFile ~/.ssh/id_rsa_github

Host bitbucket.org
	IdentityFile ~/.ssh/id_rsa_bitbucket

